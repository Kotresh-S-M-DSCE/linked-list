/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     struct ListNode *next;
 * };
 */
struct ListNode* reverseList(struct ListNode* head) {
       struct ListNode *temp=head,*prev=NULL,*ahead=NULL;
       while(temp!=NULL)
       {
          ahead=temp->next;
          temp->next=prev;
          prev=temp;
          temp=ahead;
       }
       return prev;
}